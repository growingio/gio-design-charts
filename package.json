{
  "name": "@gio-design/charts",
  "version": "2.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/charts.umd.min.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/growingio/gio-design-charts.git",
  "author": "Zhang Rui <zhangrui@growingio.com>",
  "license": "Apache-2.0",
  "scripts": {
    "extract": "formatjs extract \"src/**/*.{ts,tsx}\" --ignore \"src/{react-app-env,typings}.d.ts\" --out-file locales/gio-design-charts.json --id-interpolation-pattern \"[sha512:contenthash:base64:6]\" --format crowdin",
    "compile": "formatjs compile locales/translations/en/gio-design-charts.json --ast --out-file src/locales/en.json --format crowdin",
    "eslint": "eslint src --ext .ts,.tsx",
    "start": "react-scripts start",
    "build": "father-build",
    "preversion": "yarn build",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "stylelint": "stylelint 'src/**/*.less' --custom-syntax postcss-less",
    "all": "npm run stylelint && npm run test && npm run storybook:build && npm run build"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@antv/component": "^0.8.17",
    "@antv/data-set": "^0.11.8",
    "@antv/event-emitter": "^0.1.2",
    "@antv/g-base": "^0.5.9",
    "@antv/g2": "^4.1.29",
    "@antv/util": "^2.0.17",
    "@babel/runtime": "^7.15.4",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "re-resizable": "^6.9.1",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@formatjs/cli": "^4.7.1",
    "@gio-design/utils": "^22.0.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.171",
    "@types/node": "^17.0.5",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "babel-jest": "^27.4.5",
    "babel-loader": "8.2.3",
    "babel-plugin-formatjs": "^10.3.10",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "css-loader": "5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "father-build": "^1.21.1",
    "jest": "^27.4.5",
    "jest-canvas-mock": "^2.3.1",
    "jest-junit": "^13.0.0",
    "less": "3.13.1",
    "less-loader": "7.3.0",
    "postcss-less": "^5.0.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-docgen-typescript-plugin": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.6",
    "react-scripts": "4.0.3",
    "semantic-release": "^19.0.2",
    "style-loader": "3.3.1",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.5.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "5.67.0"
  },
  "peerDependencies": {
    "@gio-design/utils": ">=22.0.2",
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0",
    "react-intl": ">=5.24.6"
  },
  "resolutions": {
    "babel-loader": "8.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
