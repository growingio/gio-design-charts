{
  "name": "gio-chart-next",
  "version": "0.1.0",
  "private": true,
  "main": "./es/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "repository": "https://github.com/growingio/gio-design-charts.git",
  "author": "Zhang Rui <zhangrui@growingio.com>",
  "license": "Apache-2.0",
  "scripts": {
    "father-build": "father build",
    "start": "react-scripts start",
    "build": "father build",
    "gen-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "gen-version": "conventional-changelog -p angular > VERSION.md",
    "postversion": "yarn gen-version && yarn gen-changelog",
    "preversion": "git fetch --tags",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "NODE_ENV=production build-storybook -s public"
  },
  "files": [
    "dist",
    "build",
    "lib",
    "es"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@antv/g2": "^4.1.20",
    "@babel/runtime": "^7.14.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.171",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "css-loader": "1.0.0",
    "father-build": "^1.19.6",
    "less": "^4.1.1",
    "less-loader": "6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-docgen-typescript-plugin": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "style-loader": "1.3.0",
    "typescript": "^4.1.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "web-vitals": "^1.0.1",
    "webpack": "4.44.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "babel-jest": "^26.6.0",
    "prettier": "^2.3.2",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
