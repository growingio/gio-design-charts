{
  "name": "@gio-design/charts",
  "version": "1.3.11",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/charts.umd.min.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/growingio/gio-design-charts.git",
  "author": "Zhang Rui <zhangrui@growingio.com>",
  "license": "Apache-2.0",
  "scripts": {
    "gen-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "gen-version": "conventional-changelog -p angular > VERSION.md",
    "postversion": "yarn gen-version && yarn gen-changelog",
    "preversion": "git fetch --tags",
    "extract": "formatjs extract \"src/**/*.{ts,tsx}\" --ignore \"src/{react-app-env,typings}.d.ts\" --out-file locales/gio-design-charts.json --id-interpolation-pattern \"[sha512:contenthash:base64:6]\" --format crowdin",
    "compile": "formatjs compile locales/translations/en/gio-design-charts.json --ast --out-file src/locales/en.json --format crowdin",
    "eslint": "eslint src --ext .ts,.tsx",
    "start": "react-scripts start",
    "build": "father-build",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public",
    "stylelint": "stylelint 'src/**/*.{css,less}' --syntax less",
    "all": "npm run stylelint && npm run test && npm run storybook:build && npm run build"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@antv/component": "^0.8.17",
    "@antv/data-set": "^0.11.8",
    "@antv/event-emitter": "^0.1.2",
    "@antv/g-base": "^0.5.9",
    "@antv/g2": "^4.1.29",
    "@antv/util": "^2.0.17",
    "@babel/runtime": "^7.15.4",
    "@gio-design/utils": "^22.0.2",
    "date-fns": "^2.23.0",
    "lodash": "^4.17.21",
    "re-resizable": "^6.9.1",
    "react-intl": "^5.20.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@formatjs/cli": "^4.2.32",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.171",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "babel-jest": "^27.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-formatjs": "^10.3.10",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "father-build": "^1.19.6",
    "jest": "^27.1.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-junit": "^12.0.0",
    "less": "3.13.1",
    "less-loader": "6.0.0",
    "prettier": "^2.3.2",
    "react": "^16.13.0",
    "react-docgen-typescript-plugin": "^1.0.0",
    "react-dom": "^16.13.0",
    "react-scripts": "4.0.3",
    "style-loader": "1.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.1.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "4.44.2"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
